function Visualize_PSF2(X,Y,rl,aper_coords)
    % Visualize the psf
    
    % scale the coordinates to that the p
    scale = 3;
    Xs = scale*X;
    Ys = scale*Y;
    
    figure
    PSF2 = reshape(abs(MultiAperturePSF([Xs(:),Ys(:)],aper_coords)).^2,size(Xs));
    imagesc([min(Xs(:)),max(Xs(:))]/rl,[min(Ys(:)),max(Ys(:))]/rl,PSF2);
    title('Multi-Aperture |PSF|^2')
    xlabel('x [rl]')
    ylabel('y [rl]')
    axis 'square'
end